type Book {
    id: ID!
    title: String!
    author: User!
}

type User {
    id: ID!
    name: String!
    email: String!

}

type Query {
    books: [Book!]!
    users: [User!]!
}

input NewBook {
    title: String!
    userId: String!
    name: String!
}

type Mutation {
    createBook(input: NewBook!): Book!
}